package com.capgemini.gestorproyectos.model;

import java.io.Serializable;

public class FuncionalidadesDTO implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Long id;
	
	private String nombre;
	
	private ProyectoDTO proyecto;
	private GrupoTareaDTO grupoTarea;
	private EntregaDTO entrega;
	private String descripcion;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public ProyectoDTO getProyecto() {
		return proyecto;
	}
	public void setProyecto(ProyectoDTO proyecto) {
		this.proyecto = proyecto;
	}
	public GrupoTareaDTO getGrupoTarea() {
		return grupoTarea;
	}
	public void setGrupoTarea(GrupoTareaDTO grupoTarea) {
		this.grupoTarea = grupoTarea;
	}
	public EntregaDTO getEntrega() {
		return entrega;
	}
	public void setEntrega(EntregaDTO entrega) {
		this.entrega = entrega;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public FuncionalidadesDTO(Long id, String nombre, ProyectoDTO proyecto, GrupoTareaDTO grupoTarea,
			EntregaDTO entrega, String descripcion) {
		super();
		this.id = id;
		this.nombre = nombre;
		this.proyecto = proyecto;
		this.grupoTarea = grupoTarea;
		this.entrega = entrega;
		this.descripcion = descripcion;
	}
	public FuncionalidadesDTO()
	{
		
	}
	
}
