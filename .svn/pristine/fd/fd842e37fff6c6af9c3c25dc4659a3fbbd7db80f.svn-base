package com.capgemini.gestorproyectos.model;

import java.io.Serializable;

/**
 * @author capgemini
 *
 * Representacion del objeto Cliente.
 */
public class GrupoTareaDTO implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 6762880077678057248L;

	private Long id;
	
	private ItemDTO item;
	
	private ComplejidadDTO complejidad;
	
	private TamanioDTO tamanio;
	
	private ArquitecturaDTO arquitectura;
	
	public GrupoTareaDTO() {
		
	}

	/**
	 * @param id
	 * @param item
	 * @param complejidad
	 * @param tamanio
	 * @param arquitectura
	 */
	public GrupoTareaDTO(Long id, ItemDTO item, ComplejidadDTO complejidad, TamanioDTO tamanio,
			ArquitecturaDTO arquitectura) {
		this.id = id;
		this.item = item;
		this.complejidad = complejidad;
		this.tamanio = tamanio;
		this.arquitectura = arquitectura;
	}

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the item
	 */
	public ItemDTO getItem() {
		return item;
	}

	/**
	 * @param item the item to set
	 */
	public void setItem(ItemDTO item) {
		this.item = item;
	}

	/**
	 * @return the complejidad
	 */
	public ComplejidadDTO getComplejidad() {
		return complejidad;
	}

	/**
	 * @param complejidad the complejidad to set
	 */
	public void setComplejidad(ComplejidadDTO complejidad) {
		this.complejidad = complejidad;
	}

	/**
	 * @return the tamanio
	 */
	public TamanioDTO getTamanio() {
		return tamanio;
	}

	/**
	 * @param tamanio the tamanio to set
	 */
	public void setTamanio(TamanioDTO tamanio) {
		this.tamanio = tamanio;
	}

	/**
	 * @return the arquitectura
	 */
	public ArquitecturaDTO getArquitectura() {
		return arquitectura;
	}

	/**
	 * @param arquitectura the arquitectura to set
	 */
	public void setArquitectura(ArquitecturaDTO arquitectura) {
		this.arquitectura = arquitectura;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("GrupoTareaDTO [id=");
		builder.append(id);
		builder.append(", item=");
		builder.append(item);
		builder.append(", complejidad=");
		builder.append(complejidad);
		builder.append(", tamanio=");
		builder.append(tamanio);
		builder.append(", arquitectura=");
		builder.append(arquitectura);
		builder.append("]");
		return builder.toString();
	}

	
}
