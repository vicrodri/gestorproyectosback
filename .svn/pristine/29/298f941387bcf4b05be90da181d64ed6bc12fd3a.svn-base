package com.capgemini.gestorproyectos.dao;

import java.util.List;

import com.capgemini.gestorproyectos.model.ComplejidadDTO;

/**
 * DAO para las operaciones de BBDD de la tabla Compljidad
 * mapea al fichero ComplejidadMapper.xml
 * @author capgemini
 *
 */
public interface ComplejidadDAO {
	
	/**
	 * Devuelve los registros de la tabla Complejidades.
	 * 
	 * @param complejidad <ComplejidadDTO>
	 * @return lista de <Cliente>
	 */
	List<ComplejidadDTO> get(ComplejidadDTO complejidad);

	/**
	 * Crear una complejidad <ComplejidadDTO>
	 * 
	 * @param complejidad <ComplejidadDTO>
	 * @return complejidad <ComplejidadDTO> creada
	 */
	Long add(ComplejidadDTO complejidad);

	/**
	 * Actualiza un complejidad <ComplejidadDTO>
	 * 
	 * @param complejidad <ComplejidadDTO>
	 * @return
	 */
	Long update(ComplejidadDTO complejidad);

	/**
	 * elimina un complejidad <ComplejidadDTO>
	 * 
	 * @param complejidad <ComplejidadDTO>
	 * @return
	 */
	Long delete(ComplejidadDTO complejidad);

}
